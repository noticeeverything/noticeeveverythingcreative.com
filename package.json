{
	"name": "notice-everything-creative",
	"version": "2.0.0",
	"private": true,
	"scripts": {
		"cross-env": "cross-env",
		"clean.nuxt": "rimraf .nuxt",
		"dev": "cross-env NODE_ENV=development nodemon --watch ./src --ext 'ts,js' --exec ts-node -r tsconfig-paths/register --project tsconfig.server.json src/server/index.ts",
		"build.client": "nuxt build",
		"build.server": "tsc -p tsconfig.server.json",
		"build.prod": "run-s clean.nuxt build.client build.server",
		"deploy": "ts-node --project scripts/tsconfig.deploy.json scripts/deploy.ts",
		"nuxt": "nuxt",
		"start": "cross-env NODE_ENV=production node .nuxt/api/index.js",
		"post-update": "yarn upgrade --latest",
		"prod": "run-s -l build.prod start.prod",
		"rimraf": "rimraf",
		"test": "jest"
	},
	"dependencies": {
		"@nestjs/common": "7.0.7",
		"@nestjs/core": "7.0.7",
		"@nestjs/platform-express": "7.0.7",
		"@noticeeverything/nest": "3.0.0",
		"@nuxt/typescript-build": "0.6.2",
		"@nuxtjs/axios": "5.9.5",
		"@nuxtjs/vuetify": "1.9.1",
		"compression": "1.7.4",
		"config": "3.2.5",
		"consola": "2.11.3",
		"contentful": "7.13.1",
		"cross-env": "7.0.0",
		"morgan": "1.9.1",
		"node-sass": "4.13.0",
		"nuxt": "2.10.2",
		"reflect-metadata": "0.1.13",
		"rxjs": "6.4.0",
		"vue": "2.6.11",
		"vue-property-decorator": "8.3.0",
		"vue-template-compiler": "2.6.11"
	},
	"devDependencies": {
		"@babel/core": "^7.7.5",
		"@types/config": "^0.0.36",
		"@types/inquirer": "^6.5.0",
		"@types/jest": "^25.1.1",
		"@types/moment-timezone": "^0.5.12",
		"@types/shelljs": "^0.8.6",
		"@vue/cli-plugin-babel": "~4.3.0",
		"@vue/cli-plugin-e2e-cypress": "~4.3.0",
		"@vue/cli-plugin-eslint": "~4.3.0",
		"@vue/cli-plugin-router": "~4.3.0",
		"@vue/cli-plugin-typescript": "~4.3.0",
		"@vue/cli-plugin-unit-jest": "~4.3.0",
		"@vue/cli-plugin-vuex": "~4.3.0",
		"@vue/cli-service": "~4.3.0",
		"@vue/eslint-config-typescript": "^5.0.2",
		"@vue/test-utils": "1.0.0-beta.31",
		"copy-webpack-plugin": "^5.1.1",
		"inquirer": "^7.0.4",
		"jest": "^25.3.0",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.35",
		"nodemon": "^2.0.2",
		"npm-run-all": "^4.1.5",
		"sass-loader": "^8.0.0",
		"shelljs": "^0.8.3",
		"ts-jest": "^25.3.1",
		"ts-node": "^8.5.4",
		"tsconfig-paths": "^3.9.0",
		"vue-jest": "^3.0.5",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-node-externals": "^1.7.2"
	}
}
